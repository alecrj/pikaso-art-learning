{
  "name": "pikaso",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "deploy": "npm run build:web && npx vercel --prod",
    "type-check": "npx tsc --noEmit",
    "test": "echo 'Tests to be implemented'",
    "start:production": "expo start --no-dev --minify",
    "build:web:staging": "EXPO_PUBLIC_ENV=staging expo export --platform web --output-dir dist-staging",
    "build:web:production": "EXPO_PUBLIC_ENV=production expo export --platform web --minify --output-dir dist-production",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:all": "eas build --platform all --profile production",
    "deploy:staging": "npm run build:web:staging && vercel deploy --env staging",
    "deploy:production": "npm run build:web:production && vercel deploy --prod",
    "type-check:production": "npx tsc --noEmit --strict",
    "lint": "expo lint",
    "clean": "rm -rf dist dist-staging dist-production node_modules/.cache",
    "reset": "npm run clean && rm -rf node_modules && npm install",
    "doctor": "expo doctor"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.3.3",
    "@sentry/react-native": "^5.36.0",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "crypto-js": "^4.2.0",
    "expo": "53.0.12",
    "expo-av": "~15.1.6",
    "expo-blur": "^14.1.5",
    "expo-constants": "~15.4.0",
    "expo-dev-client": "~5.2.1",
    "expo-file-system": "~18.1.10",
    "expo-haptics": "~12.8.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-media-library": "^17.1.7",
    "expo-router": "~5.1.0",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "^0.30.9",
    "expo-status-bar": "~1.11.0",
    "lucide-react-native": "^0.513.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^4.14.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "^0.24.15",
    "@react-native-community/cli": "^18.0.0",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}
